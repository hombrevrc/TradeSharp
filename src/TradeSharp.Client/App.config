<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>

  <log4net>
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="log-file.txt"/>
      <appendToFile value="true"/>
      <maximumFileSize value="100KB"/>
      <maxSizeRollBackups value="5"/>
      <rollingStyle value="once"/>
      <encoding value="windows-1251"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level: %date [%thread] {1} - %message%newline"/>
      </layout>
    </appender>
    
    <appender name="Account" type="log4net.Appender.FileAppender">
      <file value="account-log.txt"/>
      <appendToFile value="true"/>
      <encoding value="windows-1251"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level: %date [%thread] {1} - %message%newline"/>
      </layout>
    </appender>
    
    <logger name="RollingFile">
      <level value="ALL"/>
      <appender-ref ref="RollingFile"/>
    </logger>
    
    <logger name="Account">
      <level value="ALL"/>
      <appender-ref ref="Account"/>
    </logger>
    
  </log4net>
   
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="tradeTcpBinding" closeTimeout="00:00:30" openTimeout="00:00:30" receiveTimeout="00:00:30" sendTimeout="00:00:30" transactionFlow="false" hostNameComparisonMode="StrongWildcard" maxBufferPoolSize="524288" maxReceivedMessageSize="100000000">
          <readerQuotas maxDepth="32" maxStringContentLength="10000000" maxArrayLength="100000000" maxBytesPerRead="4096" maxNameTableCharCount="16384"/>
          <security mode="None"/>
        </binding>
        <binding name="chatTcpBinding" closeTimeout="00:00:30" openTimeout="00:00:30" receiveTimeout="00:00:15" sendTimeout="00:00:15" transactionFlow="false" hostNameComparisonMode="StrongWildcard" maxBufferPoolSize="524288" maxReceivedMessageSize="100000000">
          <readerQuotas maxDepth="32" maxStringContentLength="10000000" maxArrayLength="100000000" maxBytesPerRead="4096" maxNameTableCharCount="16384"/>
          <security mode="None"/>
        </binding>
      </netTcpBinding>
      <basicHttpBinding>
        <binding name="simpleHttpBinding" closeTimeout="00:00:30" openTimeout="00:00:15" receiveTimeout="00:00:20" sendTimeout="00:00:30" hostNameComparisonMode="StrongWildcard" maxBufferPoolSize="524288" maxReceivedMessageSize="100000000">
          <readerQuotas maxDepth="32" maxStringContentLength="10000000" maxArrayLength="100000000" maxBytesPerRead="4096" maxNameTableCharCount="16384"/>
          <security mode="None"/>
        </binding>
        <binding name="userHttps" transferMode="Streamed" maxReceivedMessageSize="2147483647" maxBufferSize="2147483647" openTimeout="00:00:30" receiveTimeout="00:00:30" closeTimeout="00:00:30" sendTimeout="00:00:30">
          <readerQuotas maxDepth="2147483647" maxStringContentLength="2147483647" maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647"/>
          <security mode="None"/>
        </binding>
        <binding name="statisticsHttpBinding" closeTimeout="00:00:30" openTimeout="00:00:30" receiveTimeout="00:01:00" sendTimeout="00:00:30" hostNameComparisonMode="StrongWildcard" maxBufferPoolSize="524288" maxReceivedMessageSize="100000000">
          <readerQuotas maxDepth="32" maxStringContentLength="10000000" maxArrayLength="100000000" maxBytesPerRead="4096" maxNameTableCharCount="16384"/>
          <security mode="None"/>
        </binding>
      </basicHttpBinding>
      <wsDualHttpBinding>
        <binding name="OpenHttpBinding" maxReceivedMessageSize="2147483647"/>
      </wsDualHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://138.201.252.82:55059/AccountEfficiency" binding="basicHttpBinding" bindingConfiguration="statisticsHttpBinding" contract="TradeSharp.Contract.Contract.IAccountStatistics" name="IAccountStatisticsBinding"/>
      
      <endpoint address="http://138.201.252.82:55059/QuoteStorage" binding="basicHttpBinding" bindingConfiguration="userHttps" contract="TradeSharp.Contract.Contract.IQuoteStorage" name="IQuoteStorageBinding" behaviorConfiguration="Behaviors.EndpointBehavior"/>
      
      <endpoint address="http://138.201.252.82:55059/NewsStorage" binding="basicHttpBinding" bindingConfiguration="userHttps" contract="TradeSharp.Contract.Contract.INewsStorage" name="INewsStorageBinding" behaviorConfiguration="Behaviors.EndpointBehavior"/>
      
      <endpoint address="http://138.201.252.82:55050/TradeSharpDictionary" binding="basicHttpBinding" bindingConfiguration="simpleHttpBinding" contract="TradeSharp.Contract.Contract.ITradeSharpDictionary" name="ITradeSharpDictionaryBinding"/>

      <endpoint address="http://138.201.252.82:55050/TradeSharpAccount" binding="basicHttpBinding" bindingConfiguration="simpleHttpBinding" contract="TradeSharp.Contract.Contract.ITradeSharpAccount" name="ITradeSharpAccountBinding"/>
      
      <endpoint address="net.tcp://138.201.252.82:55051/TradeSharpServerTrade" binding="netTcpBinding" bindingConfiguration="tradeTcpBinding" contract="TradeSharp.Contract.Contract.ITradeSharpServerTrade" name="ITradeSharpServerTradeBinding"/>

      <endpoint address="http://138.201.252.82:55058/UpdateManager" binding="basicHttpBinding" bindingConfiguration="userHttps" contract="TradeSharp.UpdateContract.Contract.IUpdateManager" name="IUpdateManagerBinding"/>

      <endpoint address="net.tcp://138.201.252.82:55061/Chat" binding="netTcpBinding" bindingConfiguration="chatTcpBinding" contract="TradeSharp.Chat.Contract.IChat" name="OpenHttp"/>

      <endpoint address="http://138.201.252.82:55050/WalletManager" binding="basicHttpBinding" bindingConfiguration="simpleHttpBinding" contract="TradeSharp.Contract.Contract.IWalletManager" name="IWalletManagerBinding"/>

    </client>
    <behaviors>
      <endpointBehaviors>
        <behavior name="Behaviors.EndpointBehavior">
          <dataContractSerializer maxItemsInObjectGraph="2147483647"/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
  <appSettings>
    <add key="Quote.ServerPort" value="55056"/>
    <add key="Quote.ServerHost" value="138.201.252.82"/>
    <add key="SingleInstance" value="False"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
    <add key="Offline" value="false"/>
    <add key="Service.Url" value="http://138.201.252.82:8066" />
    <add key="VerbosePriceMark" value="true"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
  </startup>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri=""/>
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400"/>
      </providers>
    </roleManager>
  </system.web>
</configuration>
